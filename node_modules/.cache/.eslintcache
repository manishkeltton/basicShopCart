   </div>\n    </div>\n  );\n};\n\nexport default Form;\n","/var/www/html/hello-react/src/loginPage/useForm.js",["82"],"import { useState, useEffect } from 'react';\n\nconst useForm = (callback, validate) => {\n\n  const [values, setValues] = useState({});\n  const [errors, setErrors] = useState({});\n  const [isSubmitting, setIsSubmitting] = useState(false);\n\n  useEffect(() => {\n    if (Object.keys(errors).length === 0 && isSubmitting) {\n      callback();\n    }\n  }, [errors]);\n\n  const handleSubmit = (event) => {\n    if (event) event.preventDefault();\n    setErrors(validate(values));\n    setIsSubmitting(true);\n  };\n\n  const handleChange = (event) => {\n    event.persist();\n    setValues(values => ({ ...values, [event.target.name]: event.target.value }));\n  };\n\n  return {\n    handleChange,\n    handleSubmit,\n    values,\n    errors,\n  }\n};\n\nexport default useForm;","/var/www/html/hello-react/src/loginPage/LoginFormValidationRules.js",[],"/var/www/html/hello-react/src/loginPage/Login.js",[],"/var/www/html/hello-react/src/utils/refreshToken.js",[],"/var/www/html/hello-react/src/loginPage/serviceWorker.js",[],{"ruleId":"83","replacedBy":"84"},{"ruleId":"85","replacedBy":"86"},{"ruleId":"87","severity":1,"message":"88","line":3,"column":8,"nodeType":"89","messageId":"90","endLine":3,"endColumn":14},{"ruleId":"87","severity":1,"message":"91","line":4,"column":27,"nodeType":"89","messageId":"90","endLine":4,"endColumn":33},{"ruleId":"87","severity":1,"message":"92","line":4,"column":35,"nodeType":"89","messageId":"90","endLine":4,"endColumn":41},{"ruleId":"87","severity":1,"message":"93","line":4,"column":43,"nodeType":"89","messageId":"90","endLine":4,"endColumn":48},{"ruleId":"87","severity":1,"message":"94","line":5,"column":8,"nodeType":"89","messageId":"90","endLine":5,"endColumn":16},{"ruleId":"87","severity":1,"message":"95","line":6,"column":8,"nodeType":"89","messageId":"90","endLine":6,"endColumn":15},{"ruleId":"87","severity":1,"message"